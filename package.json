{
  "name": "prettier-plugin-prisma",
  "version": "0.14.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekkarimov/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "prettier . --check",
    "lint:fix": "prettier . --write",
    "prerelease": "yarn lint && yarn tsc && yarn build",
    "release": "np --no-2fa",
    "pretdd": "yarn build",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/prettier": "^2.3.2",
    "execa": "^5.1.1",
    "jest": "^27.0.6",
    "np": "^7.5.0",
    "prettier": "^2.3.2",
    "prettier-plugin-packagejson": "^2.2.11",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=12"
  }
}
