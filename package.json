{
  "name": "prettier-plugin-prisma",
  "version": "2.29.1",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib",
    "wasm",
    "!wasm/*.ts"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "prettier . --check",
    "prerelease": "npm ddp && npm i && npm run lint && npx tsc && npm run build",
    "release": "np --no-2fa",
    "pretdd": "npm run build",
    "tdd": "jest --watch",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "package-lock.json": "npm ddp"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/prettier": "2.3.2",
    "execa": "5.1.1",
    "husky": "7.0.2",
    "jest": "27.0.6",
    "lint-staged": "11.1.2",
    "np": "7.5.0",
    "prettier": "2.3.2",
    "prettier-plugin-packagejson": "2.2.11",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=12"
  }
}
