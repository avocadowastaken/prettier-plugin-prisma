{
  "name": "prettier-plugin-prisma",
  "version": "3.5.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib",
    "wasm",
    "!wasm/*.ts"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "prettier . --check",
    "prepare": "husky install",
    "prerelease": "npm i && npm run lint && npx tsc && npm run build",
    "release": "np --no-2fa",
    "pretdd": "npm run build",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/prettier": "2.4.1",
    "@umidbekk/configs": "0.1.0",
    "execa": "5.1.1",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "11.2.6",
    "np": "7.5.0",
    "prettier": "2.4.1",
    "prettier-plugin-packagejson": "2.2.13",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=7"
  }
}
