{
  "name": "prettier-plugin-prisma",
  "version": "4.8.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "checks": "npm run lint && npx tsc && npm test",
    "lint": "prettier . --check",
    "prepare": "husky install",
    "release": "np --test-script checks",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/prisma-fmt-wasm": "4.9.0-44.b29121153404aa8eda60ec38e41e477cc1c0ee66"
  },
  "devDependencies": {
    "@types/jest": "29.2.6",
    "@types/prettier": "2.7.2",
    "@umidbekk/configs": "0.2.0",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "np": "7.6.3",
    "prettier": "2.8.3",
    "prettier-plugin-packagejson": "2.4.0",
    "typescript": "4.9.4",
    "vscode-languageserver-protocol": "3.17.2"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "14.21.2",
    "npm": "8.19.3"
  }
}
