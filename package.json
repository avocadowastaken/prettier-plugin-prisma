{
  "name": "prettier-plugin-prisma",
  "version": "4.4.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "prettier . --check",
    "prepare": "husky install",
    "prerelease": "npm ci && npm run lint && npx tsc",
    "release": "np",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/prisma-fmt-wasm": "4.5.0-44.integration-fix-15655-cd15cc453ecedba322f3bb6dff831b949e0dd170"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/prettier": "2.7.2",
    "@umidbekk/configs": "0.2.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "np": "7.6.2",
    "prettier": "2.8.1",
    "prettier-plugin-packagejson": "2.3.0",
    "typescript": "4.9.4",
    "vscode-languageserver-protocol": "3.17.2"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=14"
  },
  "volta": {
    "node": "14.21.2",
    "npm": "8.19.3"
  }
}
