{
  "name": "prettier-plugin-prisma",
  "version": "4.12.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "checks": "npm run lint && npx tsc && npm test",
    "lint": "prettier . --check",
    "prepare": "husky install",
    "release": "npx --yes np --test-script checks",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/prisma-fmt-wasm": "4.12.0-67.659ef412370fa3b41cd7bf6e94587c1dfb7f67e7"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/prettier": "2.7.2",
    "@umidbekk/configs": "0.3.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "prettier-plugin-packagejson": "2.4.3",
    "typescript": "5.0.2",
    "vscode-languageserver-protocol": "3.17.3"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=8"
  },
  "volta": {
    "node": "14.21.2",
    "npm": "8.19.4"
  }
}
