{
  "name": "prettier-plugin-prisma",
  "version": "3.7.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "prettier . --check",
    "prepare": "husky install",
    "prerelease": "npm ci && npm run lint && npx tsc",
    "release": "np --no-2fa",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/prisma-fmt-wasm": "3.8.0-43.34df67547cf5598f5a6cd3eb45f14ee70c3fb86f"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/prettier": "2.4.2",
    "@umidbekk/configs": "0.1.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "np": "7.6.0",
    "prettier": "2.5.1",
    "prettier-plugin-packagejson": "2.2.15",
    "typescript": "4.5.4"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=7"
  }
}
