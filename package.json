{
  "name": "prettier-plugin-prisma",
  "version": "4.2.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "prettier . --check",
    "prepare": "husky install",
    "prerelease": "npm ci && npm run lint && npx tsc",
    "release": "np",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/prisma-fmt-wasm": "4.3.0-32.c875e43600dfe042452e0b868f7a48b817b9640b"
  },
  "devDependencies": {
    "@types/jest": "29.1.1",
    "@types/prettier": "2.7.1",
    "@umidbekk/configs": "0.2.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "lint-staged": "13.0.3",
    "np": "7.6.2",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.3.0",
    "typescript": "4.8.4",
    "vscode-languageserver-protocol": "3.17.2"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=7"
  }
}
