{
  "name": "prettier-plugin-prisma",
  "version": "3.10.0",
  "description": "Prettier plugin for Prisma files",
  "keywords": [
    "prettier-plugin",
    "prisma"
  ],
  "repository": "git@github.com:umidbekk/prettier-plugin-prisma.git",
  "license": "MIT",
  "author": "Umidbek Karimov",
  "main": "lib/plugin.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "prettier . --check",
    "prepare": "husky install",
    "prerelease": "npm ci && npm run lint && npx tsc",
    "release": "np",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/prisma-fmt-wasm": "3.10.0-50.73e60b76d394f8d37d8ebd1f8918c79029f0db86"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/prettier": "2.4.4",
    "@umidbekk/configs": "0.1.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "np": "7.6.0",
    "prettier": "2.5.1",
    "prettier-plugin-packagejson": "2.2.15",
    "typescript": "4.5.5",
    "vscode-languageserver-protocol": "3.16.0"
  },
  "peerDependencies": {
    "prettier": ">=2"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=7"
  }
}
